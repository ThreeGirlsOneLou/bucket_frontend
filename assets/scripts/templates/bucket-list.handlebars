<div class="row bucket-row">
  {{#each locations}}
    <div class="col-md-3">
      <div class="image-container"><img src={{this.url}}></div>
        <button type="button" class="visited" data-location-id="{{this.id}}">Visited</button>
        <button type="button" class="remove" data-location-id="{{this.id}}">Remove</button>
      <div class="location-name" hidden>
        {{this.name}}
      </div>
    </div>
  {{/each}}
</div>
